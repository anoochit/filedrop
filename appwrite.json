{
    "projectId": "filedrop",
    "projectName": "filedrop",
    "settings": {
        "services": {
            "account": true,
            "avatars": true,
            "databases": true,
            "locale": true,
            "health": true,
            "storage": true,
            "teams": true,
            "users": true,
            "functions": true,
            "graphql": true,
            "messaging": true
        },
        "auth": {
            "methods": {
                "jwt": true,
                "phone": true,
                "invites": true,
                "anonymous": true,
                "email-otp": true,
                "magic-url": true,
                "email-password": true
            },
            "security": {
                "duration": 31536000,
                "limit": 0,
                "sessionsLimit": 10,
                "passwordHistory": 0,
                "passwordDictionary": false,
                "personalDataCheck": false,
                "sessionAlerts": false,
                "mockNumbers": []
            }
        }
    },
    "buckets": [
        {
            "$id": "filedrop",
            "$permissions": [
                "create(\"users\")",
                "read(\"users\")"
            ],
            "fileSecurity": false,
            "name": "filedrop",
            "enabled": true,
            "maximumFileSize": 30000000,
            "allowedFileExtensions": [],
            "compression": "none",
            "encryption": true,
            "antivirus": true
        }
    ],
    "databases": [
        {
            "$id": "filedrop",
            "name": "filedrop",
            "enabled": true
        }
    ],
    "collections": [
        {
            "$id": "files",
            "$permissions": [],
            "databaseId": "filedrop",
            "name": "files",
            "enabled": true,
            "documentSecurity": false,
            "attributes": [
                {
                    "key": "channel",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 32,
                    "default": null
                },
                {
                    "key": "from",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 32,
                    "default": null
                },
                {
                    "key": "to",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 32,
                    "default": null
                },
                {
                    "key": "url",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                }
            ],
            "indexes": []
        },
        {
            "$id": "users",
            "$permissions": [
                "create(\"any\")",
                "create(\"users\")",
                "read(\"users\")",
                "update(\"users\")"
            ],
            "databaseId": "filedrop",
            "name": "users",
            "enabled": true,
            "documentSecurity": false,
            "attributes": [
                {
                    "key": "id",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 20,
                    "default": null
                },
                {
                    "key": "name",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 50,
                    "default": null
                },
                {
                    "key": "lat",
                    "type": "double",
                    "required": false,
                    "array": false,
                    "min": -1.7976931348623157e+308,
                    "max": 1.7976931348623157e+308,
                    "default": 13.7563
                },
                {
                    "key": "lon",
                    "type": "double",
                    "required": false,
                    "array": false,
                    "min": -1.7976931348623157e+308,
                    "max": 1.7976931348623157e+308,
                    "default": 100.5018
                },
                {
                    "key": "updated",
                    "type": "datetime",
                    "required": true,
                    "array": false,
                    "format": "",
                    "default": null
                }
            ],
            "indexes": []
        }
    ]
}